<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="off">
    <Appenders>
        <File name="appender" fileName="target/zio-kafka-test.log">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger - %msg%n"/>
        </File>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="appender"/>
        </Root>
    </Loggers>
</Configuration>